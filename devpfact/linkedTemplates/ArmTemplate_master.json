{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "devpfact"
		},
		"devkey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devkeyvau.vault.azure.net/"
		},
		"keylink_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "devsecretest"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devpfact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"devkey_properties_typeProperties_baseUrl": {
						"value": "[parameters('devkey_properties_typeProperties_baseUrl')]"
					},
					"keylink_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('keylink_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}